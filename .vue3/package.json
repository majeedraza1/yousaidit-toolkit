{
  "name": "yousaidit-toolkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.reminders-admin.config.ts",
    "build:reminders-admin": "tsc --noEmit && vite build --config vite.reminders-admin.config.ts",
    "build:reminders-frontend": "tsc --noEmit && vite build --config vite.reminders-frontend.config.ts",
    "dev:designer-dashboard": "vite --config vite.designer-dashboard.config.ts",
    "build:designer-dashboard": "tsc --noEmit && vite build --config vite.designer-dashboard.config.ts",
    "build": "npm run build:reminders-admin && npm run build:reminders-frontend"
  },
  "dependencies": {
    "@shapla/vanilla-components": "^0.4.0",
    "@shapla/vue-components": "^1.3.0",
    "axios": "^1.6.6",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}
